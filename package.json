{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:module": "cd apps/react-multiple && pnpm run build",
    "publish:module": "cd apps/react-multiple && npm publish",
    "test": "cd apps/react-multiple && pnpm run test",
    "dev": "turbo run dev",
    "dev:docs": "cd apps/docs && turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10",
  "name": "package"
}
